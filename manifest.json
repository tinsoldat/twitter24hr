{
  "manifest_version": 2,
  "name": "Twitter 24 hour-clock",
  "version": "1.0.1",
  "description": "24-hour clock format for Twitter.",
  "content_security_policy": "default-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}